        <p class="message-text message-text--error">A valid URL is required.</p>
      </div>

      <div class="field">
        <label>Number input</label>
        <input type="number" placeholder="Enter a number">
      </div>

      <div class="field">
        <label>Password input</label>
        <input type="password" placeholder="Type your password">
        <p class="message-text message-text--warning">Your password should be 6 or more characters long.</p>
      </div>

      <div class="field">
        <label>Search input</label>
        <input type="search" placeholder="Enter a search term">
      </div>

      <div class="field">
        <label>Range input</label>
        <input type="range" min="0" max="100" step="10" value="0">
      </div>

      <div class="field field--choice">
        <fieldset>
          <legend>Radio buttons</legend>

          <label for="male"><input type="radio" name="sex" id="male" value="male">Male</label>
          <label for="female"><input type="radio" name="sex" id="female" value="female">Female</label>
        </fieldset>
      </div>

      <div class="field field--choice">
        <fieldset>
          <legend>Checkboxes</legend>

          <label for="choice-1"><input type="checkbox" id="choice-1" value="choice-1">Choice 1</label>
          <label for="choice-2"><input type="checkbox" id="choice-2" value="choice-2">Choice 2</label>
        </fieldset>
      </div>

      <div class="field">
        <label>Textarea</label>
        <textarea placeholder="Enter your message here"></textarea>
      </div>

      <button class="btn">Submit</button>
    </form>

